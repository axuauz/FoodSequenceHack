{
  "manifest_version": 3,
  "name": "Food Sequence HACK",
  "version": "1.0",
  "description": "민주컷",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "https://food-sequence.vercel.app/*"
  ],
  "content_scripts": [
    {
      "matches": ["https://food-sequence.vercel.app/*"],
      "js": ["src/content.js"],
      "css": ["src/styles.css"],
      "run_at": "document_end"
    }
  ],
  "web_accessible_resources": [{
    "resources": ["src/injected.js"],
    "matches": ["https://food-sequence.vercel.app/*"]
  }],
  "icons": {
    "48": "icons/icon.png"
  }
}